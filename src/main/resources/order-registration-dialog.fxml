<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.furniture_factory.controllers.OrderController">
    <content>
        <AnchorPane>
            <DatePicker fx:id="dateCalendar" layoutX="395.0" layoutY="102.0"/>
            <TextField fx:id="shopIdTextField" layoutX="396.0" layoutY="228.0"/>
            <Label layoutX="148.0" layoutY="102.0" text="Дата"/>
            <Label layoutX="148.0" layoutY="166.0" text="Магазин"/>
            <Button layoutX="177.0" layoutY="277.0" mnemonicParsing="false" text="Сохранить" onAction="#dialogSaveButton"/>
            <Button layoutX="300.0" layoutY="277.0" mnemonicParsing="false" text="Отмена" onAction="#dialogCancelButton"/>
        </AnchorPane>
    </content>
</DialogPane>
